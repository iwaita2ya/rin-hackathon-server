<p id="notice"><%= notice %></p>

<h1>Field Record Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @field_record_details.each do |field_record_detail| %>
      <tr>
        <td><%= link_to 'Show', field_record_detail %></td>
        <td><%= link_to 'Edit', edit_field_record_detail_path(field_record_detail) %></td>
        <td><%= link_to 'Destroy', field_record_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Field Record Detail', new_field_record_detail_path %>
